/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"
#include "mt7621-pinctrl-default-gpio.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "ASUS RT-AC85P";

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	leds {
		compatible = "gpio-leds";

		power {
			label = "power";
			gpios = <&gpio0 4 GPIO_ACTIVE_LOW>;
		};

		wlan5g {
			label = "sw5g";
			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
		};

		wlan2g {
			label = "sw2g";
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		wps {
			label = "wps";
			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};


		reset {
			label = "reset";
			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

};

&bch {
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x0 0xe0000>;
				read-only;
			};

			partition@e0000 {
				label = "BootEnv";
				reg = <0xe0000 0x100000>;
				read-only;
			};

			partition@1e0000 {
				label = "Factory";
				reg = <0x1e0000 0x100000>;
				read-only;
			};

			partition@2e0000 {
				label = "factory2";
				reg = <0x2e0000 0x100000>;
				read-only;
			};

			partition@3e0000 {
				label = "firmware";
				reg = <0x3e0000 0x3000000>;
			};
			
			partition@33e0000  {
				label = "Config";
				reg = <0x33e0000 0x100000>;
			};

			partition@34e0000 {
				label = "Storage";
				reg = <0x34e0000 0x400000>;		
			};

			partition@38e0000 {
				label = "RWFS";
				reg = <0x38e0000 0x46a0000>;
			};

			partition@all {
				label = "ALL";
				reg = <0x0 0x7f80000>;
			};

		};
	};
};

&pcie {
	status = "okay";
};

&xhci {
	status = "okay";
};

&u3phy1 {
	status = "okay";
};
